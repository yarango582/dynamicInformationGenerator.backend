{
  "name": "dynamicinformationgenerator",
  "version": "1.0.0",
  "description": "send informacion at server's ftp",
  "main": "src/app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/app.ts",
    "start": "node dist/app.js",
    "build": "tsc --build",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:version": "npm run typeorm version",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "entity:create": "npm run typeorm entity:create -- -n",
    "subscriber:create": "npm run typeorm subscriber:create -- -n",
    "schema:sync": "npm run typeorm schema:sync",
    "schema:log": "npm run typeorm schema:log",
    "schema:drop": "npm run typeorm schema:drop",
    "cache:clear": "npm run typeorm cache:clear",
    "query": "npm run typeorm query"
  },
  "author": "yarangodev",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.0.4",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "morgan": "^1.10.0",
    "node": "^16.4.1",
    "nodemon": "^2.0.12",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "tslint": "^5.12.1",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "ts-node": "^10.1.0"
  }
}
